<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">

        <record model="ir.cron" id="cron_compute_so_quantities" forcecreate="True">
            <field name="name">Recompute Delivered &amp; Invoiced quantity in SO</field>
            <field name="interval_number">1</field>
            <field name="interval_type">year</field>
            <field name="numbercall">1</field>
            <field name="doall" eval="False"/>
            <field name="model" eval="'sale.order'"/>
            <field name="function" eval="'cron_update_so_quantities'"/>
            <field name="args" eval="'()'" />
            <field name="nextcall" eval="(DateTime.now() + timedelta(minutes=5)).strftime('%Y-%m-%d %H:%M:%S')"/>
        </record>

	</data>
</odoo>